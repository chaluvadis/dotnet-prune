{
  "name": "dotnet-prune-vscode",
  "displayName": "DotNetPrune",
  "description": "Visualize unused .NET code in VS Code with hierarchical tree view showing findings organized by solution, project, and files.",
  "version": "0.0.4",
  "publisher": "nomad-in-code",
  "icon": "resources/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaluvadis/dotnet-prune"
  },
  "homepage": "https://github.com/chaluvadis/dotnet-prune",
  "bugs": {
    "url": "https://github.com/chaluvadis/dotnet-prune/issues"
  },
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dotnetprune.refresh",
        "title": "DotNetPrune: Refresh Findings"
      },
      {
        "command": "dotnetprune.runAnalysis",
        "title": "DotNetPrune: Run Analysis"
      },
      {
        "command": "dotnetprune.clearFindings",
        "title": "DotNetPrune: Clear Findings"
      },
      {
        "command": "dotnetprune.openFinding",
        "title": "DotNetPrune: Open Finding"
      },
      {
        "command": "dotnetprune.copyFilePath",
        "title": "Copy File Path"
      },
      {
        "command": "dotnetprune.copyProjectName",
        "title": "Copy Project Name"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "DotNetPrune",
      "properties": {}
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "dotnetprune-views",
          "title": "DotNetPrune",
          "icon": "resources/gray.png"
        }
      ]
    },
    "views": {
      "dotnetprune-views": [
        {
          "id": "dotnetprune-findings",
          "name": "Findings",
          "icon": "resources/icon.png"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "dotnetprune.copyFilePath",
          "when": "view == dotnetprune-findings && viewItem == file",
          "group": "7_modification@1"
        },
        {
          "command": "dotnetprune.copyProjectName",
          "when": "view == dotnetprune-findings && viewItem == project",
          "group": "7_modification@1"
        }
      ]
    }
  },
  "scripts": {
    "compile": "node esbuild.js",
    "clean": "rm -rf dotnet-prune-vscode-0.0.1.vsix",
    "dotnet:publish": "dotnet publish ./FindUnused/FindUnused.csproj --output ./dist/FindUnused/",
    "package": "pnpm clean && pnpm compile && pnpm dotnet:publish && vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "24.10.1",
    "typescript-eslint": "^8.46.4",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.0"
  }
}
